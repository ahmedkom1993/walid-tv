<script>
    $(document).ready(function(){
        $('a[href="#top"]').click(function(){
            $('html, body').animate({scrollTop: 0}, 'slow');
            return false;
        });
        
        $(window).scroll(function() {
            if ($(this).scrollTop() > 100) {
                $('a[href="#top"]').fadeIn();
            } else {
                $('a[href="#top"]').fadeOut();
            }
        });
    });
</script>
